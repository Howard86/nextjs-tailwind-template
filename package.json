{
  "name": "nextjs-template",
  "version": "1.4.0",
  "engines": {
    "node": ">=18"
  },
  "description": "Opinionated Next.js template configured with TypeScript, DaisyUI, Heroicons and other devtools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Howard86/nextjs-template"
  },
  "author": "Howard Tai <howard@howardism.dev>",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "jest",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint"
  ],
  "bugs": {
    "url": "https://github.com/Howard86/nextjs-template/issues"
  },
  "homepage": "https://github.com/Howard86/nextjs-template",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "TIMING=1 eslint .",
    "format": "prettier --write --list-different .",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "daisyui": "^3.1.7",
    "next": "^13.4.7",
    "next-api-handler": "^0.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@next/bundle-analyzer": "^13.4.7",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/testing-library__jest-dom": "^5.14.8",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-howardism": "^0.1.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.2",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.3",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
